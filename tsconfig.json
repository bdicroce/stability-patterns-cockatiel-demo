{
   "compilerOptions":{ 
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": false,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "Node",
    "outDir": "dist",
    "lib": ["es6", "dom", "es2017"]
   },
   "include":[ 
      "src/**/*"
   ],
   "exclude":[ 
      "node_modules"
   ],
   "compileOnSave":false
}